<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorOSU"
    tools:context=".TeacherCreateDesks">

    <!-- TODO: Fix contstraints for page. -->

    <ImageButton
        android:id="@+id/testStudentButton"
        android:layout_width="73dp"
        android:layout_height="70dp"
        android:layout_marginTop="23dp"
        android:backgroundTint="#E91E63"
        android:contentDescription="g8IuOC8EnfhCOFaEQgv9u0ZVeGH2"
        app:layout_constraintTop_toBottomOf="@+id/teachersNameCreateDesksTextView"
        app:srcCompat="@drawable/ic_launcher_foreground"
        tools:layout_editor_absoluteX="186dp" />

    <Button
        android:id="@+id/submitAttendance"
        android:layout_width="225dp"
        android:layout_height="76dp"
        android:layout_marginTop="21dp"
        android:backgroundTint="@color/colorPrimary"
        android:text="@string/submit"
        app:layout_constraintTop_toBottomOf="@+id/seatsGridLayout"
        tools:layout_editor_absoluteX="93dp" />

    <ImageView
        android:id="@+id/teacherProfilePictureCreateDesksImageView"
        android:layout_width="106dp"
        android:layout_height="109dp"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground"
        tools:layout_editor_absoluteX="28dp" />

    <TextView
        android:id="@+id/teachersNameCreateDesksTextView"
        android:layout_width="162dp"
        android:layout_height="36dp"
        android:layout_marginTop="26dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="186dp" />

    <ImageButton
        android:id="@+id/editSeatsButton"
        android:layout_width="64dp"
        android:layout_height="62dp"
        android:background="@null"
        app:srcCompat="@android:drawable/ic_menu_manage"
        tools:layout_editor_absoluteX="284dp"
        tools:layout_editor_absoluteY="85dp" />

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/seatsGridLayout"
        android:layout_width="396dp"
        android:layout_height="427dp"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/testStudentButton"
        tools:layout_editor_absoluteX="7dp">

    </androidx.gridlayout.widget.GridLayout>

</androidx.constraintlayout.widget.ConstraintLayout>