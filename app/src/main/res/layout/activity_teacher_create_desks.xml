<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorOSU"
    tools:context=".TeacherCreateDesks">

    <!-- TODO: Fix contstraints for page. -->

    <ImageButton
        android:id="@+id/testStudentButton"
        android:layout_width="73dp"
        android:layout_height="70dp"
        android:layout_marginStart="186dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="576dp"
        android:contentDescription="g8IuOC8EnfhCOFaEQgv9u0ZVeGH2"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_popup_sync" />

    <Button
        android:id="@+id/submitAttendance"
        android:layout_width="225dp"
        android:layout_height="76dp"
        android:layout_marginTop="629dp"
        android:layout_marginBottom="26dp"
        android:backgroundTint="@color/colorPrimary"
        android:text="@string/submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/teacherProfilePictureCreateDesksImageView"
        android:layout_width="106dp"
        android:layout_height="109dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="277dp"
        android:layout_marginBottom="596dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/teachersNameCreateDesksTextView"
        android:layout_width="162dp"
        android:layout_height="36dp"
        android:layout_marginStart="186dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="669dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/editSeatsButton"
        android:layout_width="64dp"
        android:layout_height="62dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="584dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_manage" />

    <androidx.gridlayout.widget.GridLayout
        xmlns:grid="http://schemas.android.com/apk/res-auto"
        android:id="@+id/seatsGridLayout"
        android:layout_width="wrap_content"
        android:layout_height="427dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="181dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="123dp"
        app:alignmentMode="alignBounds"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        grid:useDefaultMargins="true">

    </androidx.gridlayout.widget.GridLayout>


</androidx.constraintlayout.widget.ConstraintLayout>